                "vpc": {
                    "DeploymentUnits": [
                        "vpc"
                    ],
                    "multiAZ": true,
                    "network": {
                        "RouteTables": {
                            "default": {}
                        },
                        "NetworkACLs": {
                            "Public": {
                                "Rules": {
                                    "internetAccess": {
                                        "Priority": 200,
                                        "Action": "allow",
                                        "Source": {
                                            "IPAddressGroups": [
                                                "_localnet"
                                            ]
                                        },
                                        "Destination": {
                                            "IPAddressGroups": [
                                                "_global"
                                            ],
                                            "Port": "any"
                                        },
